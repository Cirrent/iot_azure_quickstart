{
  "env": {
    "es2021": true,
    "node": true,
    "jest": true
  },
  "extends": "eslint:recommended",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-undef": "error",
    "strict": "error",
    "no-unused-vars": [
      "error",
      {
        "args": "none"
      }
    ],
    "no-duplicate-imports": "error"
  }
}
